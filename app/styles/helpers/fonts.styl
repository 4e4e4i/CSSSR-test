@font-face
	font-family "PT Sans"
	font-weight 400
	font-style normal
	src local("PT_Sans-Regular"),
		url("../fonts/PT_Sans-Regular.woff2") format("woff2"),
		url("../fonts/PT_Sans-Regular.woff") format("woff")

@font-face
	font-family "PT Sans"
	font-weight 700
	font-style: normal
	src local("PT_Sans-Bold"),
		url("../fonts/PT_Sans-Bold.woff2") format("woff2"),
		url("../fonts/PT_Sans-Bold.woff") format("woff")
